<document xmlns="http://maven.apache.org/changes/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
  <properties>
    <title>Bit Repository Message XML change notes</title>
  </properties>
  <body>
    <release version="23" date="2012-10-26" description="Made response text optiona">
      <action dev="MSS" type="updated" issue="BITMAG-770">
        Made response text optional and change purpose to indicate that the ResponseText element should only
        be used for information not found in other elements.
      </action>
      <action dev="MSS" type="add" issue="BITMAG-741" description="Adding paging functionality">
        Added the paging functionality based on timestamping to the GetAuditTrail, GetChecksum and GetFileID
        operations.
      </action>
      <action dev="Kim" type="add" description="Added FileID element to IdentifyPillarsForPutFileResponse">
        Added FileID element to IdentifyPillarsForPutFileResponse message.
      </action>
    </release>
    <release version="22" date="2012-10-04" description="Adding 'REQUEST_NOT_UNDERSTOOD' response code ">
      <action dev="MSS" type="add" issue="BITMAG-734" >
        Added the 'REQUEST_NOT_UNDERSTOOD' response code to the protocol.
      </action>
    </release>
    <release version="20" date="2012-09-04" description="Minor extension to the AlarmCode">
      <action dev="JOLF" type="add" issue="BITMAG-702">
        Added the 'INTEGRITY_ISSUE' AlarmCode to the protocol.
      </action>
    </release>
    <release version="19" date="2012-08-01" description="Update for versioning support">
      <action dev="Kim" type="Update">
        Updated a bit of documentation in relation to ReplaceFileFinalResponse
      </action>
      <action dev="Kim" type="Update">
        Update of packaging, so that versioning related stuff is included
      </action>
    </release>
  </body>
</document>